{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/foomo/posh-providers/filosottile/mkcert",
  "$ref": "#/$defs/Config",
  "$defs": {
    "Certificate": {
      "properties": {
        "name": {
          "type": "string"
        },
        "names": {
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "Config": {
      "properties": {
        "certificatePath": {
          "type": "string"
        },
        "certificates": {
          "items": {
            "$ref": "#/$defs/Certificate"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object"
    }
  }
}