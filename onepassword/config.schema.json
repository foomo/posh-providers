{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/foomo/posh-providers/onepassword/op",
  "$ref": "#/$defs/Config",
  "$defs": {
    "Config": {
      "properties": {
        "account": {
          "type": "string"
        },
        "tokenFilename": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "account",
        "tokenFilename"
      ]
    }
  }
}